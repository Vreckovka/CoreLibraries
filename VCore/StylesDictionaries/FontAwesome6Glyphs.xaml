    </ResourceDictionary.MergedDictionaries>



    <Style x:Key="FontAwesomeGlyphPathButtonStyle"
           BasedOn="{StaticResource {x:Type controls:PathButton}}"
           TargetType="{x:Type controls:PathButton}">
        <Setter Property="FontSize" Value="15" />
        <Setter Property="IconHeight" Value="0" />
        <Setter Property="IconWidth" Value="0" />
        <Setter Property="IconMargin" Value="0" />
        <Setter Property="IsReadOnly" Value="True" />
        <Setter Property="ForegroundDefaultColor" Value="#757575" />
        <Setter Property="ForegroundCheckedColor" Value="{StaticResource YellowColor}" />
        <Setter Property="FontFamily" Value="{StaticResource FontAwesome6Pro_Regular}" />
    </Style>

